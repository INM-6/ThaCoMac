                                        "properties": {
                                            "TT_Direction": {
                                                "type": "string",
                                                "title": "Tracing Direction:",
                                                "enum": ["Click to select", "Anterograde", "Retrograde"],
                                                "default": "Click to select"
                                            },
                                            "If_Ipsilateral": {
                                                "type": "string",
                                                "title": "If Ipsilateral?",
                                                "enum": ["Y", "N"],
                                                "default": "Y"
                                            },
                                            "LabeledSite_Name_by_Author": {
                                                "type": "string",
                                                "title": "LabeledSite Name by Author:",
                                                "minLength": 1
                                            },
                                            "Source_Text_of_LabeledSite": {
                                                "type": "string",
                                                "title": "Source Text about LabeledSite:",
