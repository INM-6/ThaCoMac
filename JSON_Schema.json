                "Comment_on_TPS": {
                    "type": "string",
                    "title": "Comment on Thalamic Parcellation Scheme:"
                },
                "PS_for_Certain_Brainsites": {
                    "type": "array",
                    "title": "Parcellation Scheme for Certain Brainsites",
                    "items": {
                        "type": "object",
                        "title": "Case",
                        "properties": {
                            "Brainsite": {
                                "type": "string",
                                "title": "Brainsite:"
                            },
                            "Parcellation_Scheme": {
                                "type": "string",
                                "title": "Parcellation Scheme:",
                                "enum": [
