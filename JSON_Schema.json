                "Comment_on_CPS": {
                    "type": "string",
                    "title": "Comment on Cortical Pacellation Scheme:"
                },
                "Thalamus_Parcellation_Scheme": {
                    "type": "string",
                    "title": "Thalamic Parcellation Scheme:",
                    "enum": [
                        "NA", 
                        "Olszewski_1952",
                        "Paxinos_1999", 
                        "Paxinos_2000",
                        "Pandya_1982",
                        "Seltzer_1984",
