{
    "title": "Article ID",
    "type": "object",
    "properties": {
        "Article_Info": {
            "type": "object",
            "title": "Article Information",
            "properties":{
                "DH_INDEX": {
                    "type": "integer",
                    "title": "DH_INDEX:",
                    "minimum": 0,
                    "maximum": 10979,
                    "default": -1
                },
                "DOI-URL": {
                    "type": "string",
                    "title": "DOI-URL:",
                    "default": "NA",
                    "minLength": 1
                },
                "PMID": {
                    "type": "string",
                    "title": "PMID:",
                    "pattern": "[0-9]{8}|NA",
                    "default": "NA",
                    "minLength": 1
                },
                "PMCID": {
                    "type": "string",
                    "title": "PMCID:",
                    "pattern": "PMC[0-9]{7}|NA",
                    "default": "NA",
                    "minLength": 1
                },
                "Title": {
                    "type": "string",
                    "title": "Article Title:",
                    "default": "NA",
                    "minLength": 1
                },
                "First_Author": {
                    "type": "string",
                    "title": "First Autor (FirstName LastName):",
                    "default": "NA",
                    "minLength": 1                
                },
                "Publication_Year": {
                    "type": "integer",
                    "title": "Publication Year:",
                    "minimum": 1936,
                    "maximum": 2023,
                    "default": -1
                },
                "Comment_on_Article_Info": {
                    "type": "string",
                    "title": "Comment on Article Information:"
                }
            },
            "required": [
                "DH_INDEX", 
                "DOI-URL", 
                "PMID", 
                "PMCID", 
                "Title", 
                "First_Author", 
                "Publication_Year", 
                "Comment_on_Article_Info"
            ]
        },
        "Parcellation_Scheme": {
            "type": "object",
            "title": "Parcellation Scheme",
