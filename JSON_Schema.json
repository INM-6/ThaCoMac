{
    "title": "Literature Metadata",
    "type": "object",
    "properties": {
        "Article": {
            "title": {
                "type": "string",
                "description": "This is generated automatically from the previous two fields",
                "template": "{{DH_INDEX}}_{{DOI}}_{{PMID}}",
                "watch": {
                  "city": "location.city",
                  "state": "location.state"
                }
            },
            "type": "object",
            "description": "..",
            "properties": {
                "Aiticle_IDENTIFIER": {
                    "title": "Aiticle_IDENTIFIER",
                    "type": "object",
                    "description": "DH331_DOI",
                    "properties":{
                        "DH_INDEX": {
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 10799
                        },
                        "DOI": {
                            "type": "string",
                            "description": "DOI"
                        },
                        "PMID": {
                            "type": "string",
                            "description": "PMID"
                        },
                        "PMCID": {
                            "type": "string",
                            "description": "PMCID"
                        },
                        "Title": {
                            "type": "string",
                            "description": "Title"
                        }
                    }
                },
                "Article_Info": {
                    "title": "Article_Info",
                    "type": "object",
                    "description": "Article_Info",
                    "properties":{
                        "URL": {
                            "type": "string",
                            "description": "DH_INDEX"
                        },
                        "Full_Text_URL": {
                            "type": "string",
                            "description": "DOI"                        },
                        "Journal": {
                            "type": "string",
                            "description": "PMID"                        },
                        "Publication_Year": {
                            "type": "string",
                            "description": "PMCID"                        },
                        "First_Autor": {
                            "type": "string",
                            "description": "Title"                        }
                    }
                },
                "Macaque_Species": {
                    "type": "array",
                    "format": "table",
                    "title": "Macaque_Species",
                    "uniqueItems": true,
                    "items": {
                        "type": "object",
                        "title": "Macaque_Specie",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": [
                                    "cat",
                                    "dog",
                                    "bird",
                                    "reptile",
                                    "other"
                                ]                            
                            },
                            "name": {
                                "type": "string"
                            }
                        }
                    },
                    "default": [
                        {
                            "type": "NA",
                            "name": "Walter"
                        }
                    ]
                },
                "Parcellation_Scheme_and_Nomenclature": {
                    "title": "Parcellation_Scheme_and_Nomenclature",
                    "type": "object",
                    "description": "Parcellation_Scheme_and_Nomenclature",
                    "properties":{
                        "Atlas": {
                            "type": "string",
                            "description": "Atlas"
                        },
                        "Cortex_Pacellation_Scheme": {
                            "type": "string",
                            "description": "Nomenclature"
                        },
                        "Thalamus_Parcellation_Scheme": {
                            "type": "string",
                            "description": "Parcellation_Scheme_and_Nomenclature"
                        },
                        "Nomenclature": {
                            "type": "string",
                            "description": "Nomenclature"
                        }
                    }
                },
                "Injection_and_Label": {
                    "title": "Injection_and_Label",
                    "type": "object",
                    "description": "Injection_and_Label",
                    "properties":{
                        "Injection_ID": {
                            "title": "Injection_ID",
                            "type": "object",
                            "description": "Injection_Method",
                            "properties":{
                                "TT_Substance": {
                                    "type": "string",
                                    "description": "Injection_Method",
                                    "enum": [
                                        "male",
                                        "female"
                                    ]
                                },
                                "TT_Direction": {
                                    "type": "string",
                                    "description": "Injection_Volume",
                                    "enum": [
                                        "antrograte",
                                        "retrograte"
                                    ]
                                },
                                "Injection_Area_by_Author": {
                                    "type": "string",
                                    "description": "Injection_Location"
                                },
                                "Injection_Area_ID_by_DD99": {
                                    "type": "string",
                                    "description": "Injection_Location"
                                },
                                "Labeled_Area": {
                                    "type": "object",
                                    "description": "Labeled_Area",
                                    "properties":{
                                        "Labeled_Area_ID": {
                                            "type": "object",
                                            "description": "Injection_Location",
                                            "properties":{
                                                "Labeled_Area_ID": {
                                                    "type": "string",
                                                    "description": "Injection_Location"
                                                },
                                                "Labeled_Area_ID_by_Author": {
                                                    "type": "string",
                                                    "description": "Injection_Location"
                                                },
                                                "Labeled_Area_ID_by_DD99": {
                                                    "type": "string",
                                                    "description": "Injection_Location"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}