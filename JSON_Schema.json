                "Thalamus_Parcellation_Scheme",
                "Comment_on_TPS",
                "PS_for_Certain_Brainsites"
            ]
        },
        "TT_Experiments": {
            "type": "array",
            "title": "Tract-Tracing Experiments",
            "items": {
                "type": "object",
                "title": "Subject Experiment",
                "properties": {
                    "Subject_Subspecies": {
                        "type": "string",
                        "title": "Subspecies of The Subject:",
                        "enum": [
                            "Macaque (not specified)",
                            "Rhesus macaque (Macaca mulatta)",
                            "Japanese macaque (Macaca fuscata)",
                            "Pig-tailed macaque (Macaca nemestrina)",
                            "Bonnet macaque (Macaca radiata)",
                            "Crab-eating (cynomolgus) macaque (Macaca fascicularis)"
                        ]
                    },
                    "Comment_on_Experimental_Subject": {
                        "type": "string",
                        "title": "Comment on This Experimental Subject:",
                        "default": "NA"
                    },
                    "Injection_and_Labeling": {
                        "type": "array",
                        "title": "Injection and Labeling",
                        "items": {
                            "type": "object",
                            "title": "Injection",
                            "properties": {
                                "Injection_Info": {
                                    "type": "object",
                                    "title": "Injection Information:",
                                    "properties": {
                                        "Tracer": {
                                            "type": "string",
                                            "title": "Tracer:",
                                            "enum": [
                                                "Click to select", 
                                                "Alexa Fluor",
                                                "AA",
                                                "Leucine",
                                                "Proline",
                                                "AVV",
                                                "BDA",
                                                "BrdU",
                                                "CTX",
                                                "CTB",
                                                "DY",
                                                "DR",
                                                "FB",
                                                "Fluorescent",
                                                "FE",
                                                "FS",
                                                "PI",
                                                "Fucose",
                                                "FR",
                                                "FR-dextran amine",
                                                "FG",
                                                "HRP",
                                                "HRP-WGA",
                                                "NY",
                                                "DY/NY",
                                                "LY",
                                                "PHA",
                                                "GB",
                                                "Thymidine",
                                                "WGA",
                                                "WGA-HRP"
                                            ],
                                            "default": "Click to select"
                                        },
                                        "InjectionSite_Name_by_Author": {
                                            "type": "string",
                                            "title": "InjectionSite Name by Author:",
                                            "minLength": 1
                                        },
                                        "InjectionSite_Info_Source_Text": {
                                            "type": "string",
                                            "title": "Text about InjectionSite:",
                                            "minLength": 1
                                        },
                                        "InjectionSite_Info_Source_Figure": {
                                            "type": "string",
                                            "title": "Figure of Information Source:",
                                            "default": "NA",
                                            "minLength": 1
                                        },
                                        "InjectionSite_ID_by_D99": {
                                            "type": "string",
                                            "title": "Injection Site ID by D99:",
                                            "enum": ["Click to select", "Area name 1", "Area name 2"],
                                            "default": "Click to select"
                                        },
                                        "Injection_Coverage": {
                                            "type": "object",
                                            "title": "Injection Coverage:",
                                            "properties": {
                                                "Cover_Entire_Region": {
                                                    "type": "string",
                                                    "title": "Injection covered the entire region?",
                                                    "enum": ["Click to select", "NA", "Y", "N"],
                                                    "default": "Click to select"
                                                },
                                                "Cover_Adjacent_Regions": {
                                                    "type": "string",
                                                    "title": "Injection covered adjacent regions?",
                                                    "enum": ["Click to select", "NA", "Y", "N"],
                                                    "default": "Click to select"
                                                },
                                                "Extend_into_White_Matter": {
                                                    "type": "string",
                                                    "title": "Injection extended into white matter?",
                                                    "enum": ["Click to select", "NA", "Y", "N"],
                                                    "default": "Click to select"
                                                },
                                                "Injection_Coverage_Info_Source_Text": {
                                                    "type": "string",
                                                    "title": "Text about Injection Coverage:",
                                                    "minLength": 1
                                                },
                                                "Comment": {
                                                    "type": "string",
                                                    "title": "Comment:"
                                                }
                                            },
                                            "required": [
                                                "Cover_Entire_Region", 
                                                "Cover_Adjacent_Regions", 
                                                "Extend_into_White_Matter", 
                                                "Comment"
                                            ]
                                        },
                                        "InjectionSite_Mapping_Confidence": {
                                            "type": "string",
                                            "title": "Mapping confidence of the injection site:",
                                            "enum": ["Click to select", "High", "Medium", "Low"],
                                            "default": "Click to select"
                                        },
                                        "Comment_on_InjectionSite_Mapping": {
                                            "type": "string",
                                            "title": "Comment on InjectionSite Mapping:"
                                        },
                                        "Comment_on_Injection": {
                                            "type": "string",
                                            "title": "Comment on Injection:" 
                                        }
                                    },
                                    "required": [
                                        "Tracer", 
                                        "InjectionSite_Name_by_Author", 
                                        "InjectionSite_Info_Source_Text", 
                                        "InjectionSite_Info_Source_Figure", 
                                        "InjectionSite_ID_by_D99", 
                                        "Injection_Coverage", 
                                        "InjectionSite_Mapping_Confidence", 
                                        "Comment_on_InjectionSite_Mapping", 
                                        "Comment_on_Injection"
                                    ]
                                },
                                "LabeledSites": {
                                    "type": "array",
                                    "title": "List of LabeledSites:",
                                    "items": {
                                        "type": "object",
                                        "title": "LabeledSite",
                                        "properties": {
                                            "TT_Direction": {
                                                "type": "string",
                                                "title": "Tracing Direction:",
                                                "enum": ["Click to select", "Anterograde", "Retrograde"],
                                                "default": "Click to select"
                                            },
                                            "If_Ipsilateral": {
                                                "type": "string",
                                                "title": "If Ipsilateral?",
                                                "enum": ["Y", "N"],
                                                "default": "Y"
                                            },
                                            "LabeledSite_Name_by_Author": {
                                                "type": "string",
                                                "title": "LabeledSite Name by Author:",
                                                "minLength": 1
                                            },
                                            "LabeledSite_Info_Source_Text": {
                                                "type": "string",
                                                "title": "Text about LabeledSite:",
                                                "minLength": 1
                                            },
                                            "LabeledSite_Info_Source_Figure": {
                                                "type": "string",
                                                "title": "Figure of LabeledSite:",
                                                "default": "NA",
                                                "minLength": 1
                                            },
                                            "LabeledSite_ID_by_D99": {
                                                "type": "string",
                                                "title": "LabeledSite ID by D99:",
                                                "enum": ["Click to select", "Area ID 1", "Area ID 2", "Area ID 3"],
                                                "default": "Click to select"
                                            },
                                            "Coverage_of_LabeledSite": {
                                                "type": "string",
                                                "title": "Coverage of LabeledSite:",
                                                "enum": ["Click to select", "NA", "All over the specified region", "Concentrated in a unnamed subarea"],
                                                "default": "Click to select"
                                            },
                                            "LabeledSite_Coverage_Info_Source_Text": {
                                                "type": "string",
                                                "title": "Text about LabeledSite Coverage:",
                                                "minLength": 1
                                            },
                                            "Qualitative_Strength_of_Labeling": {
                                                "type": "string",
                                                "title": "Strength of Labeling in Qualitative Terms:",
                                                "enum": ["Click to select", "NA", "Strong", "Medium", "Weak"],
                                                "default": "Click to select"
                                            },
                                            "Source_of_Connection_Strength": {
                                                "type": "string",
                                                "title": "Text about Connection Strength:",
                                                "minLength": 1
                                            },
                                            "FLN": {
                                                "type": "number",
                                                "title": "Fraction of Labeled Neurons (FLN):",
                                                "minimum": -1,
                                                "maximum": 1,
                                                "default": -1
                                            },
                                            "LabeledSite_Mapping_Confidence": {
                                                "type": "string",
                                                "title": "Mapping confidence of the labeled site:",
                                                "enum": ["Click to select", "High", "Medium", "Low"],
                                                "default": "Click to select"
                                            },
                                            "Comment_on_LabeledSite_Mapping": {
                                                "type": "string",
                                                "title": "Comment on LabeledSite Mapping:"
                                            },
                                            "Comment_on_LabeledSite": {
                                                "type": "string",
                                                "title": "Comment on LabeledSite:"
                                            }
                                        },
                                        "required": [
                                            "TT_Direction", 
                                            "If_Ipsilateral", 
                                            "LabeledSite_Name_by_Author", 
                                            "LabeledSite_Info_Source_Text", 
                                            "LabeledSite_Info_Source_Figure", 
                                            "LabeledSite_ID_by_D99", 
                                            "Coverage_of_LabeledSite", 
                                            "Qualitative_Strength_of_Labeling", 
                                            "Source_of_Connection_Strength", 
                                            "FLN", 
                                            "LabeledSite_Mapping_Confidence", 
                                            "Comment_on_LabeledSite_Mapping", 
                                            "Comment_on_LabeledSite"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "Injection_Info", 
                                "LabeledSites"
                            ]
                        }
                    }
                },
                "required": [
                    "Subject_Subspecies", 
                    "Comment_on_Experimental_Subject", 
                    "Injection_and_Labeling"
                ]
            }
        },
        "Form_of_Data_in_General": {
            "type": "string",
            "title": "In which form are data provided in general:",
            "enum": [
                "Click to select", "NA", "Only Text", "Text and Figures"
            ],
            "default": "Click to select"
        },
        "Comment_on_Article": {
            "type": "string",
            "title": "Comments on This Article:"
        }
    },
    "required": [
        "Article_Info", 
        "Parcellation_Scheme", 
        "TT_Experiments", 
        "Form_of_Data_in_General", 
        "Comment_on_Article"
    ]
}