        "TT_Experiments": {
            "type": "array",
            "title": "Tract-Tracing Experiments",
            "items": {
                "type": "object",
                "title": "Subject Experiment",
                "properties": {
                    "Subject_Subspecies": {
                        "type": "string",
                        "title": "Subspecies of The Subject:",
                        "enum": [
                            "Macaque (not specified)",
                            "Rhesus macaque (Macaca mulatta)",
                            "Japanese macaque (Macaca fuscata)",
                            "Pig-tailed macaque (Macaca nemestrina)",
                            "Bonnet macaque (Macaca radiata)",
                            "Crab-eating (cynomolgus) macaque (Macaca fascicularis)"
                        ]
                    },
                    "Source_Text_of_This_Experiemenal_Subject": {
                        "type": "string",
                        "title": "Source Text of This Experimental Subject:",
                        "default": "NA"
                    },
                    "Comment_on_This_Experimental_Subject": {
                        "type": "string",
                        "title": "Comment on This Experimental Subject:",
                        "default": "NA"
                    },
                    "Injection_and_Labeling": {
                        "type": "array",
