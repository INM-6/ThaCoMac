                                                "type": "string",
                                                "title": "Source Figure of LabeledSite Coverage:",
                                                "default": "NA",
                                                "minLength": 2
                                            },
                                            "Qualitative_Strength_of_Labeling": {
                                                "type": "string",
                                                "title": "Strength of Labeling in Qualitative Terms:",
                                                "enum": ["Click to select", "NA", "Strong", "Medium", "Weak"],
                                                "default": "Click to select"
                                            },
                                            "Source_of_Connection_Strength": {
                                                "type": "string",
                                                "title": "Source about Connection Strength:",
                                                "default": "NA",
                                                "minLength": 2
                                            },
                                            "FLN": {
                                                "type": "number",
                                                "title": "Fraction of Labeled Neurons (FLN):",
                                                "minimum": -1,
                                                "maximum": 1,
                                                "default": -1
                                            },
                                            "LabeledSite_Mapping_Confidence": {
                                                "type": "string",
                                                "title": "Mapping confidence of the labeled site:",
                                                "enum": ["Click to select", "High", "Medium", "Low"],
                                                "default": "Click to select"
                                            },
                                            "Comment_on_LabeledSite_Mapping": {
                                                "type": "string",
                                                "title": "Comment on LabeledSite Mapping:"
                                            }
                                        },
                                        "required": [
                                            "TT_Direction", 
                                            "If_Ipsilateral", 
                                            "LabeledSite_Name_by_Author", 
                                            "Source_Text_of_LabeledSite", 
                                            "Source_Figure_of_LabeledSite", 
                                            "Comment_on_LabeledSite", 
                                            "LabeledSite_ID_by_D99", 
                                            "Coverage_of_LabeledSite", 
                                            "Source_Text_of_LabeledSite_Coverage", 
                                            "Source_Figure_of_LabeledSite_Coverage", 
                                            "Qualitative_Strength_of_Labeling", 
                                            "Source_of_Connection_Strength", 
                                            "FLN", 
                                            "LabeledSite_Mapping_Confidence", 
                                            "Comment_on_LabeledSite_Mapping"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "Injection_Info", 
                                "LabeledSites"
                            ]
                        }
                    }
                },
                "required": [
                    "Subject_Subspecies", 
                    "Source_Text_of_This_Experiemenal_Subject", 
                    "Comment_on_This_Experimental_Subject", 
                    "Injection_and_Labeling"
                ]
            }
        },
        "Form_of_Data_in_General": {
            "type": "string",
            "title": "In which form are data provided in general:",
            "enum": [
                "Click to select", "NA", "Only Text", "Text and Figures"
            ],
            "default": "Click to select"
