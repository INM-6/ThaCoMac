                "Comment_on_TPSR": {
                    "type": "string",
                    "title": "Comment on Thalamic Parcellation Scheme Reference:"
                },
                "PSR_for_Certain_Cortical_Regions": {
                    "type": "array",
                    "title": "Parcellation Scheme for Certain Cortical Regions:",
                    "items": {
                        "type": "object",
                        "title": "Case",
                        "properties": {
                            "Region_Name": {
                                "type": "string",
                                "title": "Region Name:",
                                "default": "To be recorded",
                                "minLength": 3
                            },
                            "CPSR": {
                                "type": "array",
                                "title": "Cortical Parcellation Scheme Reference:",
                                "items": {
                                    "type": "string",
                                    "title": "Reference",
                                    "enum": [
                                        "To be recorded", 
                                        "And so on"
                                    ],
                                    "default": "To be recorded"
                                }
                            },
                            "Source_Description": {
                                "type": "string",
                                "title": "Source Description:",
                                "default": "To be recorded",
                                "minLength": 3
                            },
                            "Comment": {
                                "type": "string",
                                "title": "Comment:"
                            }
