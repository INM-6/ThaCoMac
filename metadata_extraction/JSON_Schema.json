{
    "title": "Article ID",
    "type": "object",
    "properties": {
        "Comment_on_This_Article": {
            "type": "string",
            "title": "Comment on This Article:"
        },
        "Form_of_Data_Provided": {
            "type": "string",
            "title": "In which form are data provided in general?",
            "enum": [
                "To be recorded",
                "Only Text",
                "Text and Figures"
            ],
            "default": "To be recorded"
        },
        "Contralateral_Connection_Reported": {
            "type": "string",
            "title": "Does this article report contralateral connections?",
            "enum": [
                "To be recorded",
                "Yes",
                "No"
            ],
            "default": "To be recorded"
        },
        "Article_Info": {
            "type": "object",
            "title": "Article Information",
            "properties":{
                "DH_INDEX": {
                    "type": "integer",
                    "title": "DH_INDEX:",
                    "minimum": 0,
                    "maximum": 10979,
                    "default": -1
                },
                "DOI-URL": {
                    "type": "string",
                    "title": "DOI-URL:",
                    "default": "https://doi.org/",
                    "minLength": 17
                },
                "PMID": {
                    "type": "string",
                    "title": "PMID:",
                    "pattern": "[0-9]{8}|NA",
                    "default": "NA",
                    "minLength": 2
                },
                "PMCID": {
                    "type": "string",
                    "title": "PMCID:",
                    "pattern": "PMC[0-9]{7}|NA",
                    "default": "NA",
                    "minLength": 2
                },
                "Article_Title": {
                    "type": "string",
                    "title": "Article Title:",
                    "default": "NA",
                    "minLength": 3
                },
                "First_Author": {
                    "type": "string",
                    "title": "First Autor (FirstName_LastName):",
                    "default": "NA",
                    "minLength": 3                
                },
                "Publication_Year": {
                    "type": "integer",
                    "title": "Publication Year:",
                    "minimum": 1936,
                    "maximum": 2024,
                    "default": -1
                },
                "Comment_on_Article_Info": {
                    "type": "string",
                    "title": "Comment on Article Information:"
                }
            },
            "required": [
                "DH_INDEX", 
                "DOI-URL", 
                "PMID", 
                "PMCID", 
                "Article_Title", 
                "First_Author", 
                "Publication_Year", 
                "Comment_on_Article_Info"
            ]
        },
